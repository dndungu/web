<?xml version="1.0" encoding="UTF-8"?>
<form name="permissionmap" title="permissionmap.label" primarykey="ID">
	<field name="site" type="int" length="20">
		<element name="site" type="text" label="site.label" placeholder="site.placeholder"></element>
	</field>
	<field name="role" type="int" length="20">
		<element name="role" type="text" label="role.label" placeholder="role.placeholder"></element>
	</field>
	<field name="permission" type="int" length="5">
		<element name="permission" type="text" label="permission.label" placeholder="permission.placeholder"></element>
	</field>
	<field name="creationTime" type="int" length="10">
		<element name="creationTime" type="text" label="creationTime.label" placeholder="creationTime.placeholder"></element>
	</field>
	<field name="submit">
		<element name="action" type="submit" value="action.submit"></element>
	</field>
	<field name="reset">
		<element name="action" type="reset" value="action.reset"></element>
	</field>
</form>