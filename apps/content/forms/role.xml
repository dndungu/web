<?xml version="1.0" encoding="UTF-8"?>
<form name="role" title="role.label" primarykey="ID">
	<field name="site" type="int" length="20">
		<element name="site" type="text" label="site.label" placeholder="site.placeholder"></element>
	</field>
	<field name="title" type="varchar" length="32">
		<element name="title" type="text" label="title.label" placeholder="title.placeholder"></element>
	</field>
	<field name="isSystem" type="enum" length="'Yes'" precision="'No'">
		<element name="isSystem" type="text" label="isSystem.label" placeholder="isSystem.placeholder"></element>
	</field>
	<field name="creationTime" type="int" length="10">
		<element name="creationTime" type="text" label="creationTime.label" placeholder="creationTime.placeholder"></element>
	</field>
	<field name="inTrash" type="enum" length="'Yes'" precision="'No'">
		<element name="inTrash" type="text" label="inTrash.label" placeholder="inTrash.placeholder"></element>
	</field>
	<field name="submit">
		<element name="action" type="submit" value="action.submit"></element>
	</field>
	<field name="reset">
		<element name="action" type="reset" value="action.reset"></element>
	</field>
</form>